<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name=" test suite"  parallel="tests" thread-count="5">
    <!-- parallel="tests" thread-count="3" -->

    <listeners>
        <listener class-name="com.kodlanir.listeners.ExtentReportListener"/>
        <listener class-name="com.kodlanir.listeners.AllureReportListener"></listener>
    </listeners>


    <test name="Login Tests">

        <classes>
            <class name="com.kodlanir.tests.LoginTests"/>
        </classes>

    </test>

    <test name="Register Tests ">
        <classes>
            <class name="com.kodlanir.tests.RegisterTests"></class>
        </classes>
    </test>


</suite>

